<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FastScrollRecyclerView">
        <attr name="CanFastScroll" format="boolean"></attr>
        <attr name="VerticalThumbDrawable" format="reference"></attr>
        <attr name="VerticalTrackDrawable" format="reference"></attr>
        <attr name="HorizontalThumbDrawable" format="reference"></attr>
        <attr name="HorizontalTrackDrawable" format="reference"></attr>
        <attr name="ScrollerPaddingTop" format="reference|dimension"></attr>
        <attr name="ScrollerPaddingBottom" format="reference|dimension"></attr>
        <attr name="ScrollerPaddingLeft" format="reference|dimension"></attr>
        <attr name="ScrollerPaddingRight" format="reference|dimension"></attr>
        <attr name="MinScreenCountScrollerShow" format="integer"></attr>
        <attr name="popBackGroundColor" format="color|reference"/>
        <attr name="popTextColor" format="color|reference"/>
        <attr name="popTextSize" format="dimension|reference"/>

    </declare-styleable>

</resources>